{
  "name": "navi-cloud",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "deploy:router": "wrangler deploy --config wrangler.router.toml",
    "tail": "wrangler tail",
    "db:create": "wrangler d1 create navi-cloud-registry",
    "db:migrate": "wrangler d1 execute navi-cloud-registry --file=./migrations/001_init.sql"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241218.0",
    "typescript": "^5.3.3",
    "wrangler": "^3.99.0"
  }
}
